  "buildOptions": {
    "output": {
      "globals": {
        "axios": "axios"
      },
      "name": "LgdUtilsAxios"
    },
    "_cjs": {
      "_extraExternal": [
        "axios/lib/core/buildFullPath",
        "axios/lib/helpers/buildURL"
      ]
    },
    "_esm": {
      "_extraExternal": [
        "axios/lib/core/buildFullPath",
        "axios/lib/helpers/buildURL"
      ]
    },
    "_global": {
      "external": [
        "axios"
      ]
    }
  },
  "dependencies": {
    "@lgd-utils/object": "^0.0.10",
    "@lgd-utils/utils": "^0.0.12",
    "@lgd-utils/validate": "^0.0.13",
    "axios": "^0.23.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0"
  },
  "description": "A axios adapter and middleware tool",
  "devDependencies": {
    "@types/lodash": "^4.14.176",
    "@types/lru-cache": "^5.1.1"
  },
  "directories": {
    "lib": "src",
    "test": "__tests__",
    "types": "types"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "homepage": "https://github.com/LGDHuaOPER/lgd-utils/tree/main/packages/axios#readme",
  "jsdelivr": "dist/axios.global.prod.js",
  "keywords": [
    "axios",
    "http",
    "request",
    "response",
    "middleware",
    "adapter",
    "cache"
  ],
  "license": "MIT",
  "main": "index.js",
  "module": "dist/axios.esm.js",
  "name": "@lgd-utils/axios",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LGDHuaOPER/lgd-utils.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "types": "dist/axios.d.ts",
  "unpkg": "dist/axios.global.prod.js",
  "version": "0.0.1"
}
